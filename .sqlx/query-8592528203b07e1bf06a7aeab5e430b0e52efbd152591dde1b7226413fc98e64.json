  "db_name": "PostgreSQL",
  "query": "\n    SELECT\n      au.uid,\n      COALESCE(au.name, au.email) AS name,\n      au.email,\n      au.metadata ->> 'icon_url' AS avatar_url,\n      acm.permission_id\n    FROM public.af_collab_member acm\n    JOIN public.af_user au ON acm.uid = au.uid\n    WHERE acm.oid = $1\n    ORDER BY acm.created_at ASC\n    ",
